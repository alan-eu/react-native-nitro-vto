material {
    name : camera_feed,
    shadingModel : unlit,
    vertexDomain : device,
    depthWrite : false,
    depthCulling : false,
    requires : [
        uv0
    ],
    parameters : [
        {
            type : samplerExternal,
            name : cameraTexture
        }
    ]
}

fragment {
    void material(inout MaterialInputs material) {
        prepareMaterial(material);
        vec2 uv = getUV0();
        material.baseColor = texture(materialParams_cameraTexture, uv);
    }
}
